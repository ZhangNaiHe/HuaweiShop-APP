<view class="welcome-bottom">
<van-tabbar active="{{ active }}" bind:change="onChange">
  <van-tabbar-item bindtap="toWElcome">
    <image
      slot="icon"
      src="{{ icon.normal }}"
      mode="aspectFit"
      style="width: 30px; height: 18px;"
    />
    <image
      slot="icon-active"
      src="{{ icon.active }}"
      mode="aspectFit"
      style="width: 30px; height: 18px;"
    />
    <text class="welcome-bottom-one">首页</text>
    
  </van-tabbar-item>
   <van-tabbar-item bindtap="toclassify">
    <image
      slot="icon"
      src="{{ aicon.normal }}"
      mode="aspectFit"
      style="width: 30px; height: 18px;"
    />
    <image
      slot="icon-active"
      src="{{ aicon.active }}"
      mode="aspectFit"
      style="width: 30px; height: 18px;"
    />
    分类
  </van-tabbar-item>
  <van-tabbar-item bindtap="toMe">
    <image
      slot="icon"
      src="{{ bicon.normal }}"
      mode="aspectFit"
      style="width: 30px; height: 18px;"
    />
    <image
      slot="icon-active"
      src="{{ bicon.active }}"
      mode="aspectFit"
      style="width: 30px; height: 18px;"	
    />
    我的
  </van-tabbar-item>
</van-tabbar>
